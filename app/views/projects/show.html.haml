- title "Project"

%p
  %strong Title:
  = @project.title
%p
  %strong Client:
  = @project.client
%p
  %strong Caption:
  = @project.caption
%p
  %strong Description:
  = @project.description
%p
  %strong Image:
  = image_tag @project.image_url.to_s
%p
  %strong Image:
  = image_tag @project.logo_url.to_s
%p
  = link_to "Edit", edit_project_path(@project)
  |
  = link_to "Destroy", @project, :confirm => 'Are you sure?', :method => :delete
  |
  = link_to "View All", projects_path
